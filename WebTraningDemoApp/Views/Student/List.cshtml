@model List<WebTraningDemoApp.Models.StudentModel>


    <table>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Studing Level</th>
        </tr>

             @foreach(StudentModel student in Model)
             {
                 <tr>
                     <td>@student.Name</td>
                     <td>@student.Email</td>
                     <td>@student.Level</td>
               </tr>
             }

    </table>


  
